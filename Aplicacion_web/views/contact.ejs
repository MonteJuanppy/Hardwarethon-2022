<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('partials/head'); %>
    </head>

    <body>
        <%- include('partials/header'); %>

        <main>
            <div class="container-fluid">
                <div class ="row">
                    <div class ="col-sm-12 col-md-4 col-lg-4 col-xl-4 py-2">
                        <div class="jumbotron">
                            <h1>Contacts</h1>
                            <ul><% team.forEach(function(team) 
                                { %>
                                    <li>
                                    <strong><%= team.name %></strong> representing <%= team.organization %>, born <%= team.birth_year %>
                                    </li>
                                <% }); %>
                             </ul>
                             <p><%= tagline %></p>
                        </div>
                    </div>
                    <div class ="col-sm-12 col-md-8 col-lg-8 col-xl-8 py-2">
                        <img src="logo.gif" class="img-fluid">
                    </div>
                </div>

                <div class ="row">
                    <div class ="col-sm-10 col-md-10 col-lg-10 col-xl-10 py-1">
                    </div>
                </div>
            </div>
        </main>

        <footer>
            <%- include('partials/footer'); %>
        </footer>
    </body>
</html>

